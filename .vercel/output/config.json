{
  "version": 3,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ],
  "images": {
    "sizes": [256, 384, 640, 750, 828, 1080, 1200],
    "minimumCacheTTL": 60,
    "formats": ["image/webp", "image/avif"]
  },
  "crons": []
}
